# Ones that have their own macros.
X11/Xlib.h	AC_PATH_X
alloca.h        AC_FUNC_ALLOCA
dirent.h	AC_HEADER_DIRENT
float.h		AC_HEADER_STDC
float.h		AC_CHECK_HEADERS
ndir.h		AC_HEADER_DIRENT
stdarg.h	AC_HEADER_STDC
